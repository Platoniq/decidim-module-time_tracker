<div id="questionnaires">
  <div class="card pt-8">
    <div class="item_show__header">
      <h1 class="item_show__header-title">
        <%= t(".title") %>
        <div class="button--title">
          <%= export_dropdown %>
        </div>
      </h1>
    </div>

    <div class="card-section">
      <div class="container">
        <%= render partial: "decidim/time_tracker/admin/questionnaires/questionnaire", locals: {
          questionnaire: time_tracker.activity_questionnaire,
          edit_path: edit_activity_questionnaire_path,
          title: t("tasks.activity_questionnaire.title", scope: "decidim.time_tracker.admin")
        } %>
        <%= render partial: "decidim/time_tracker/admin/questionnaires/questionnaire", locals: {
          questionnaire: time_tracker.assignee_questionnaire,
          edit_path: edit_assignee_questionnaire_path,
          title: t("tasks.assignee_questionnaire.title", scope: "decidim.time_tracker.admin")
        } %>
      </div>
    </div>
  </div>
</div>
